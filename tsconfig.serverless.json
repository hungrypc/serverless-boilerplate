{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "module": "ES6",
    "target": "es2017",
    "noImplicitAny": false,
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "declaration": false,
    "declarationMap": false,
    "listFiles": false
  }
}
